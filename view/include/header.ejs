<style>
    .head-tab {
        padding: 0px 10px;
        text-align: center;
        line-height: 44px;
        background: #efeff0;
        border: 1px solid #dddddd;
        position: relative;
    }

    .head-tab i {
        bottom: -1px;
    }

    .head-tab i::before {
        content: "";
        display: block;
        height: 2px;
        width: 34px;
        margin: 0px auto;
        background: #777;
    }

    .head-tab a {
        display: block;
        margin: 0px auto;
        width: 2em;
    }

    .header-checkin {
        position: absolute;
        left: 0px;
        top: 0px;
        width: 44px;
        height: 44px;
        background-image: url("data:image/png;base64,");
        background-repeat: no-repeat;
        background-position: center;
        background-size: 25px;
    }

    .swipe-tab {
        position: relative;
        overflow: hidden;
        z-index: 1;
    }

    .swipe-tab i {
        position: absolute;
        left: 0px;
        transform: 0.3 ease-out;
        -webkit-transform: -webkit-transform .3s ease-out;
    }

    .swipe-tab a {
        float: left;
        text-align: center;
        font-size: 18px;
        -webkit-tap-transform-color: rgba(0, 0, 0, 0);
        font-family: '楷体';
    }

    .swipe-tab a.actove {
        font-weight: 600;
        color: #282828;
    }

